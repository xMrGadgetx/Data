math.randomseed(tick())

local blurEffect = Instance.new("BlurEffect")
local BansheLoading = Instance.new("ScreenGui")
local BansheLogo = Instance.new("ImageLabel")
local soundEffect = Instance.new("Sound")

local function loadBanshe()
	local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
	
	BansheLoading.Name = math.random(1, 385)
	BansheLoading.Parent = game.CoreGui
	BansheLoading.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	
	BansheLogo.Name = math.random(1, 385)
	BansheLogo.Parent = BansheLoading
	BansheLogo.AnchorPoint = Vector2.new(0.5, 0.5)
	BansheLogo.BackgroundColor3 = Color3.new(1, 1, 1)
	BansheLogo.BackgroundTransparency = 1
	BansheLogo.BorderSizePixel = 0
	BansheLogo.Position = UDim2.new(0.5, 0, 0.55, 0)
	BansheLogo.Size = UDim2.new(0.723361611, 0, 0.413349509, 0)
	BansheLogo.Image = "rbxassetid://130425102"
	BansheLogo.ImageTransparency = 1
	BansheLogo.ScaleType = Enum.ScaleType.Fit
	
	UIAspectRatioConstraint.Name = math.random(1, 385)
	UIAspectRatioConstraint.Parent = BansheLogo
	UIAspectRatioConstraint.AspectRatio = 2.2044892311096
	
	blurEffect.Name = math.random(1, 385)
	blurEffect.Parent = game:GetService("Lighting")
	blurEffect.Size = 0
	
	soundEffect.Name = math.random(1, 385)
	soundEffect.Parent = game.CoreGui
	soundEffect.SoundId = "rbxassetid://541909867"
	soundEffect.Volume = 2
	
	soundEffect:Play()
	
	spawn(function()
		for i = 1, 20 do
			blurEffect.Size = blurEffect.Size + 1
			wait()
		end
	end)
	
	spawn(function()
		for i = 1, 10 do
			BansheLogo.ImageTransparency = BansheLogo.ImageTransparency - 0.1
			wait()
		end
	end)
	
	spawn(function()
		BansheLogo:TweenPosition(UDim2.new(0.5, 0, 0.5, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 1)
	end)
	
	wait(2)
	
	spawn(function()
		for i = 1, 10 do
			BansheLogo.ImageTransparency = BansheLogo.ImageTransparency + 0.1
			wait()
			blurEffect.Enabled = false
		end
	end)
	
	wait(1)
	
	soundEffect:Destroy()
	BansheLoading:Destroy()
end

loadBanshe()
